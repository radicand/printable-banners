# Printable Banners Environment Variables

# Development server port (optional)
# VITE_PORT=3000

# API base URL (for future API integration)
# VITE_API_BASE_URL=https://api.printable-banners.com

# Analytics tracking (for future analytics integration)  
# VITE_ANALYTICS_ID=

# Feature flags (for future feature toggling)
# VITE_ENABLE_ADVANCED_TEMPLATES=false
# VITE_ENABLE_CUSTOM_FONTS=false

# Debug flags (development only)
# VITE_DEBUG_MODE=false
# VITE_ENABLE_CONSOLE_LOGS=false
